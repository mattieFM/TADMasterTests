  },
  "homepage": "https://github.com/mattieFM/TADMasterTests#readme",
  "devDependencies": {
    "testcafe": "^3.5.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0"
  }
}
